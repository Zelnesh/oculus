<!DOCTYPE html>
<html lan="eng">
<head>
  <title>{%block titl%}{%endblock%}</title>
  <!-- Favicon -->
   <link rel="icon" type="image/x-icon" href="{{ url_for('static', filename='images/Oculus_favicon.png') }}">
  <!-- Background image -->
  <style>body {background-image: url("{{ url_for('static', filename='images/Oculus_background.png') }}"); background-size: cover; background-repeat: no-repeat; background-position: center center; background-attachment: fixed;}</style>
  <!-- Cyber text css -->
  <link rel="stylesheet" href="{{ url_for('static', filename='cyber_text.css') }}">
  <!-- Cyber navbar css -->
  <link rel="stylesheet" href="{{ url_for('static', filename='cyber_navbar.css') }}">
  <!-- Bootstrap 5.3 CSS -->
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
  <div class="collapse" id="navbarToggleExternalContent" data-bs-theme="dark">
  <div class="bg-dark p-4">
    <h5 class="text-body-emphasis h4">OCULUS</h5>
    <span class="text-body-secondary"><a href="{{ url_for('main') }}">Main</a></span><br>
    <span class="text-body-secondary"><a href="{{ url_for('dns_lookup') }}">DNS Lookup</a></span><br>
    <span class="text-body-secondary"><a href="{{ url_for('ipg_location') }}">IP Geolocation</a></span><br>
    <span class="text-body-secondary"><a href="{{ url_for('port_scanner')}}">Port Scanner</a></span><br>
    </div>
  </div>
  <nav class="navbar navbar-dark bg-dark">
    <div class="container-fluid">
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarToggleExternalContent" aria-controls="navbarToggleExternalContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
      </button>
    </div>
  </nav>

  <!-- Main content -->
  <div class="containr mt-4">
    {%block content%}{%endblock%}
  </div>

  <!-- Bootstrap 5.3 JS bundle (includes Popper) -->
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>
